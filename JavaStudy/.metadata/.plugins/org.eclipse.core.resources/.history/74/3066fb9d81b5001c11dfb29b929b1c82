package hhd;

import java.util.Scanner;

public class Main {
	// 프로그램의 시작 점 : 리팩토링
	// 다국어 기능이 탑재된 자판기로 업그레이드
	public static void main(String[] args) {
		
		VendingMachine vm = new VendingMachine(new Japanese()); // 객체생성
		
		vm.inputMoney();
		
		vm.showMenu();
	
		
		vm.returnChange();

		
		
		

	}

}

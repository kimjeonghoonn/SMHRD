package hhd;

public class Japanese extends Language {

	@Override
	public String getTitleMSG() {
		return "1234";
	}

	@Override
	public String getInputMoneyMSG() {
		return "1234";
	}

	@Override
	public String getShowMenu1() {
		return "1234";
	}

	@Override
	public String getShowMenu2() {
		return "1.물(700) 2.콜라(1000) 3.커피(500) >>";
	}

	@Override
	public String getReturnCharge1() {
		return "1234";
	}

	@Override
	public String getReturnCharge2(int money) {
		return "1234" + money + "1234";
	}
 
	
	
}
